@model List<ResultCommentDto>

<h3 class="mb-5">6 Yorum</h3>

<ul class="comment-list">
    @foreach(var item in Model)
    {
        <li class="comment">
            <h3>@item.Name</h3>
            <div class="meta">@item.CreatedDate</div>
            <p>@item.Description</p>
        </li>
    }
</ul>