@model List<ResultCommentDto>

<h3 class="mb-5">@ViewBag.commentCount Yorum</h3>

<ul class="comment-list">
    @foreach(var item in Model)
    {
        <li class="comment">
            <h5>@item.Name</h5>
            <div class="meta">@item.CreatedDate.ToShortDateString()</div>
            <p>@item.Description</p>
        </li>
    }
</ul>